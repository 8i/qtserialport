if (isEqual(PWD, $$[QT_INSTALL_DATA]/mkspecs/features)) {
    INCLUDEPATH += $$[QT_INSTALL_HEADERS]/QtAddOnSerialPort
    LIBS        += -L$$[QT_INSTALL_LIBS] -lQtAddOnSerialPort
} else {
    INCLUDEPATH += $$PWD/../../include/QtAddOnSerialPort
    LIBS        += -L$$SERIALPORT_PROJECT_ROOT/src/ -lQtAddOnSerialPort
}

